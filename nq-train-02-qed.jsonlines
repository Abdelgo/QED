hacker"], "annotation": [{"context_entity_text": "Watch Dogs 2", "context_entity_span": [29, 41], "question_entity_text": "watch dogs 2", "question_entity_span": [22, 34]}], "sentence_starts": [0, 370, 534, 715, 788, 840, 929, 1216, 1354, 1413, 1531, 1666], "answer_type": "single_sentence"}
